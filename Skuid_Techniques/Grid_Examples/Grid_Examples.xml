			<jsitem location="inline" name="SampleData" cachelocation="false" url="">((skuid) =&gt; {
	skuid.events.subscribeOnce("pageload", () =&gt; {

        //load first model
        const model1 = skuid.model.getModel('SampleData1');
        for (var i = 1; i &lt; 11; i++){
            var newRow = model1.createRow({
                doAppend: (true), 
                additionalConditions: [{
                    field: "height",
                    value: Math.floor(Math.random() * 325) + 50,
                },{
                    field: "count",
                    value: (i * 3) - 2,
                }]
            })
        }      
        //load second model
        const model2 = skuid.model.getModel('SampleData2');
        for (var i = 1; i &lt; 11; i++){
            var newRow = model2.createRow({
                doAppend: (true), 
                additionalConditions: [{
                    field: "height",
                    value: Math.floor(Math.random() * 325) + 50,
                },{
                    field: "count",
                    value: (i * 3) - 1,
                }]
            })
        }	

        //load third model
        const model3 = skuid.model.getModel('SampleData3');
        for (var i = 1; i &lt; 11; i++){
            var newRow = model3.createRow({
                doAppend: (true), 
                additionalConditions: [{
                    field: "height",
                    value: Math.floor(Math.random() * 325) + 50,
                },{
                    field: "count",
                    value: (i * 3),
                }]
            })
        }	
	
	});
})(skuid)</jsitem>
